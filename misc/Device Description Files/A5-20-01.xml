			</properties>
			<configParameters>EnOceanConfig</configParameters>
			<variables>maint_ch_values</variables>
		</function>
		<function xmlns="https://homegear.eu/xmlNamespaces/FunctionGroupType" channel="1" type="EnOceanValveDrive">
